{
  "Pipeline": {
    "LlmProvider": {
      "DefaultProvider": "Ollama",
      "OllamaEndpoint": "http://ollama-service:11434",
      "DefaultChatModel": "deepseek-v3.1:671b-cloud",
      "DefaultEmbeddingModel": "nomic-embed-text",
      "RequestTimeoutSeconds": 180
    },
    "VectorStore": {
      "Type": "Qdrant",
      "ConnectionString": "${VECTOR_STORE_CONNECTION_STRING}",
      "BatchSize": 200,
      "DefaultCollection": "pipeline_vectors_prod"
    },
    "Execution": {
      "MaxTurns": 5,
      "MaxParallelToolExecutions": 10,
      "EnableDebugOutput": false,
      "ToolExecutionTimeoutSeconds": 60
    },
    "Observability": {
      "EnableStructuredLogging": true,
      "MinimumLogLevel": "Warning",
      "EnableMetrics": true,
      "EnableTracing": true,
      "ApplicationInsightsConnectionString": "${APPLICATION_INSIGHTS_CONNECTION_STRING}"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
